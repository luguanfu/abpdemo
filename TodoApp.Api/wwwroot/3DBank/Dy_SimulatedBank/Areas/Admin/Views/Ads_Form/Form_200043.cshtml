                    <label>
                        <input vl-default="" id="check_token" name="check_token" type="checkbox"> 令牌
                    </label>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-md-6">
        <div class="form-group">
            <label id="lbl_onlinebank" name="lbl_onlinebank" for="inputEmail3" class="col-sm-3 control-label">网上银行</label>
            <div class="col-sm-6">
                <div class="checkbox">
                    <label>
                        <input vl-default="" id="check_shortMessage" name="check_shortMessage" type="checkbox"> 短信
                    </label>
                    <label>
                        <input vl-default="" id="check_token1" name="check_token1" type="checkbox"> 令牌
                    </label>
                    <label>
                        <input vl-default="" id="check_USBKey" name="check_USBKey" type="checkbox"> USBKey
                    </label>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-md-6">
        <div class="form-group">
            <label id="lbl_dailyTransfer" name="lbl_dailyTransfer" for="inputEmail3" class="col-sm-3 control-label">转账日累计限额/笔数</label>
            <div class="col-sm-6">
                <input vl-default="" id="txt_dailyTransfer" name="txt_dailyTransfer" type="text" class="form-control input-sm">
            </div>
        </div>
    </div>
    <div class="col-md-6">
        <div class="form-group">
            <label id="lbl_yearTransfer" name="lbl_yearTransfer" for="inputEmail3" class="col-sm-3 control-label">转账年累计限额</label>
            <div class="col-sm-6">
                <input vl-default="" id="txt_yearTransfer" name="txt_yearTransfer" type="text" class="form-control input-sm">
            </div>
        </div>
    </div>
    <div class="col-md-6">
        <div class="form-group">
            <label id="lbl_dailyPayment" name="lbl_dailyPayment" for="inputEmail3" class="col-sm-3 control-label">支付日累计限额/笔数</label>
            <div class="col-sm-6">
                <input vl-default="" id="txt_dailyPayment" name="txt_dailyPayment" type="text" class="form-control input-sm">
            </div>
        </div>
    </div>
    <div class="col-md-6">
        <div class="form-group">
            <label id="lbl_yearPayment" name="lbl_yearPayment" for="inputEmail3" class="col-sm-3 control-label">支付年累计限额</label>
            <div class="col-sm-6">
                <input vl-default="" id="txt_yearPayment" name="txt_yearPayment" type="text" class="form-control input-sm">
            </div>
        </div>
    </div>
</div>
<script src="~/Scripts/bootstrap-table.min.js"></script>
<script>
    $(function () {
        initTableValue();
    });
    function initTableValue() {
        var defaultValue = $("#TabDefaultValue").val();
        if (defaultValue != null && defaultValue.length > 0) {
            fanxian();
        } else {
            setTimeout("initTableValue()", 100);
        }
    }

    var data = [];
    function setTableValue(values, id) {
        var vals = values.split('\n');
        data = [{
            'signingChannel': vals[0],
            'signingStatus': vals[1],
            'signingVerson': vals[2],
            'customerStatus': vals[3],
            'signingDate': vals[4],
            'signingOrgan': vals[5],
            'signingPhone': vals[6],
        }];

        $("#tbl").bootstrapTable('load', data);
    }
    $('#tbl').bootstrapTable({
        data: data,
        columns: [
            {
                title: '序号',
                align: 'center',
                formatter: function (value, row, index) {
                    return index + 1;
                }
            },
            {
                title: '签约渠道',
                field: 'signingChannel',
                align: 'center'
            },
            {
                title: '签约状态',
                field: 'signingStatus',
                align: 'center'
            },
            {
                title: '签约版本',
                field: 'signingVerson',
                align: 'center',
            },
            {
                title: '客户状态',
                field: 'customerStatus',
                align: 'center',
            },
            {
                title: '签约时间',
                field: 'signingDate',
                align: 'center'
            },
            {
                title: '签约机构',
                field: 'signingOrgan',
                align: 'center'
            },
            {
                title: '签约手机号',
                field: 'signingPhone',
                align: 'center'
            },
        ]
    });
    function doSubmit() {
        goPage("200011&backform=200043")
    }
</script>