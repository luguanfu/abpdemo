<script src="~/Scripts/bootstrap-table.min.js"></script>
<script>
    var data = [];
    function setTableValue(values) {
        var vals = values.split('\n');
        data = [{
            'accountSign': vals[0],
            'account': vals[1],
            'productName': vals[2],
            'accountGrade': vals[3],
            'openingOrgan': vals[4],
        }];
        $("#tbl").bootstrapTable('load', data);
    }
    $('#tbl').bootstrapTable({
        data: data,
        columns: [
            {
                checkbox: true,
                align: 'center',
            },
            {
                title: '序号',
                align: 'center',
                formatter: function (value, row, index) {
                    return index + 1;
                }
            },
            {
                title: '账户标识',
                field: 'accountSign',
                align: 'center'
            },
            {
                title: '账(卡)号',
                field: 'account',
                align: 'center'
            },
            {
                title: '产品名称',
                field: 'productName',
                align: 'center'
            },
            {
                title: '账户等级',
                field: 'accountGrade',
                align: 'center'
            },
            {
                title: '开户机构',
                field: 'openingOrgan',
                align: 'center'
            },
        ]
    });

    function doSubmit() {
        goPage("200011&backform=200029")
    }
</script>