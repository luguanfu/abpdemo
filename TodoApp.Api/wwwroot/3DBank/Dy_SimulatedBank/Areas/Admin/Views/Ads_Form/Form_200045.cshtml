<script>
    setOptions(['cashItems', 'abstract', 'accountType2', 'accountNature2', 'currency', 'currency2', 'remittance', 'remittance2', 'currency3', 'remittance3', 'currency4', 'remittance4', 'voucherType', 'abstract2', 'abstract3']);
    var data = [];
    function setTableValue(values) {
        var vals = values.split('\n');
        data = [{
            'depositAmount': vals[0],
            'cashItems': vals[1],
            'abstract': vals[2],
            'remark': vals[3],
        }];
        $("#tbl").bootstrapTable('load', data);
    }
    $('#tbl').bootstrapTable({
        data: data,
        columns: [
            {
                checkbox: true,
                align: 'center',
            },
            {
                title: '序号',
                align: 'center',
                formatter: function (value, row, index) {
                    return index + 1;
                }
            },
            {
                title: '存入金额',
                field: 'depositAmount',
                align: 'center'
            },
            {
                title: '现金项目',
                field: 'cashItems',
                align: 'center'
            },
            {
                title: '摘要',
                field: 'abstract',
                align: 'center',
            },
            {
                title: '备注',
                field: 'remark',
                align: 'center',
            },
        ]
    });
    function doSubmit() {
        checkEmpowe();
    }
    $(function () {
        initData("200015");
    })
</script>