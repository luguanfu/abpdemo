<!--电子银行签约管理[账户管理]-->
<table class="table table-hover" id="tbl"></table>
<div id="DivTabValue" style="display:none;">
    <div class="divider divider-horizontal"><div class="divider-text is-left"><strong>反显值</strong></div></div>
<textarea rows="5" id="tb1Value" name="tb1Value" style="width:100%;"></textarea>
</div>
<div class="row">
    <div class="col-md-9">
        <div class="form-group">
            <label for="inputEmail3" class="col-sm-2 control-label">业务类型</label>
            <div class="col-sm-9">
                <button type="button" class="btn btn-default" onclick="goPage('200029')">签约管理</button>
                <button type="button" class="btn btn-default" onclick="goPage('200034')">登录密码重置</button>
                <button type="button" class="btn btn-default" onclick="goPage('200043')">认证方式及限额</button>
                <button type="button" class="btn btn-default active" onclick="goPage('200046')">账户管理</button>
            </div>
        </div>
    </div>
</div>
<table class="table table-hover" id="tbl1"></table>
<div id="DivTabValue" style="display:none;">
    <div class="divider divider-horizontal"><div class="divider-text is-left"><strong>反显值</strong></div></div>
<textarea rows="5" id="tb2Value" name="tb2Value" style="width:100%;"></textarea>
</div>

<script src="~/Scripts/bootstrap-table.min.js"></script>

<script>
    $(function () {
        initTableValue();
    });
    function initTableValue() {
        var defaultValue = $("#TabDefaultValue").val();
        if (defaultValue != null && defaultValue.length > 0) {
            fanxian();
        } else {
            setTimeout("initTableValue()", 100);
        }
    }

    var data = [];
    function setTableValue(values, id) {
        console.log(values);
        var vals = values.split('\n');
        if (id == "") {
            id = "tb1Value";
        }
        if (id == "tb1Value") {
            data = [{
                'signingChannel': vals[0],
                'signingStatus': vals[1],
                'signingVerson': vals[2],
                'customerStatus': vals[3],
                'signingDate': vals[4],
                'signingOrgan': vals[5],
                'signingPhone': vals[6],
            }];
        } else {
            data = [{
                'accountSign': vals[0],
                'account': vals[1],
                'mobileStatus': vals[2],
                'onlineStatus': vals[3],
                'productName': vals[4],
                'accountGrade': vals[5],
                'openingOrgan': vals[6],
            }];
        }
        var tabId = "tbl";
        if (id == "tb2Value") {
            tabId = "tbl1";
        }

        $("#" + tabId).bootstrapTable('load', data);
    }
    $('#tbl').bootstrapTable({
        data: data,
        columns: [
            {
                title: '序号',
                align: 'center',
                formatter: function (value, row, index) {
                    return index + 1;
                }
            },
            {
                title: '签约渠道',
                field: 'signingChannel',
                align: 'center'
            },
            {
                title: '签约状态',
                field: 'signingStatus',
                align: 'center'
            },
            {
                title: '签约版本',
                field: 'signingVerson',
                align: 'center',
            },
            {
                title: '客户状态',
                field: 'customerStatus',
                align: 'center',
            },
            {
                title: '签约时间',
                field: 'signingDate',
                align: 'center'
            },
            {
                title: '签约机构',
                field: 'signingOrgan',
                align: 'center'
            },
            {
                title: '签约手机号',
                field: 'signingPhone',
                align: 'center'
            },
        ]
    });
    $('#tbl1').bootstrapTable({
        data: data,
        columns: [
            {
                checkbox: true,
                align: 'center',
            },
            {
                title: '序号',
                align: 'center',
                formatter: function (value, row, index) {
                    return index + 1;
                }
            },
            {
                title: '账户标识',
                field: 'accountSign',
                align: 'center'
            },
            {
                title: '账(卡)号',
                field: 'account',
                align: 'center'
            },
            {
                title: '手机银行状态',
                field: 'mobileStatus',
                align: 'center',
            },
            {
                title: '网上银行状态',
                field: 'onlineStatus',
                align: 'center',
            },
            {
                title: '产品名称',
                field: 'productName',
                align: 'center'
            },
            {
                title: '账户等级',
                field: 'accountGrade',
                align: 'center'
            },
            {
                title: '开户机构',
                field: 'openingOrgan',
                align: 'center'
            },
        ]
    });
    function doSubmit() {

    }
</script>