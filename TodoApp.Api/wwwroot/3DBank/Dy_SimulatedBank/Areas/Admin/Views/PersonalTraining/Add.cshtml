            <label for="inputEmail3" class="col-sm-2 control-label"><span class="text-danger">*</span>通过实训考核能获得能力项：</label>
            <div class="col-sm-10">
                <div style="text-align: left;" class=" control-label">
                    <span class="m-l-md">
                        <span class="m-r-xs">
                            <input type="radio" class="i-checks" name="E_IsTimeBonus" id="E_IsTimeBonusB" checked="checked" value="0">
                        </span>关闭
                    </span>
                    <span class="m-r-xs">
                        <input type="radio" onchange="if(/\D/.test(this.value)){layer.msg('只能输入数字');this.value='';}" class="i-checks" name="E_IsTimeBonus" id="E_IsTimeBonusA"  value="1">
                    </span>开启

                </div>

            </div>
        </div>*@

                </div>
                <!--系统统一分隔符-->
                <div class="hr-line-dashed"></div>
                <div class="form-group m-t-md">
                    <label class="col-sm-2 control-label"></label>
                    <div class="col-sm-10">
                        <button type="button" class="btn btn-w-m btn-success m-r-lg" onclick="AddBtnSubim()">保存</button>
                        <button type="button" class="btn btn-w-m btn-default" onclick="AddFormRest()">返回</button>
                    </div>
                </div>

            </form>
        </div>

    </div>
</div>

<script src="~/JS/Admin/PersonalTraining/Add.js"></script>