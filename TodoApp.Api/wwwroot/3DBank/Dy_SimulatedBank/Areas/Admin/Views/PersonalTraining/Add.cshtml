@{
    
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
    if (Request["AllType"] == "1")
    {
        ViewData["nvc"] = "个人实训考核";
    }
    else
    {
        ViewData["nvc"] = "团队实训考核";
    }
}
<script src="~/Scripts/My97DatePicker/WdatePicker.js"></script>
<link href="~/CSS/chosen/chosen.css" rel="stylesheet" />
<script src="~/Scripts/chosen/chosen.jquery.js"></script>

<div class="row   white-bg dashboard-header">

    <div class="col-sm-12 m-t-sm m-b-lg text-left">
        <button class="btn  btn-lg  btn-success m-r-lg">基本信息</button>
        <i class="fa fa-arrow-circle-right fa-2x m-r-xs"></i>

        <button class="btn   btn-lg  btn-outline btn-success m-l-lg m-r-lg" onclick="bet()">考核内容</button>


    </div>
    <div class="ibox-content">

        <div class="row">

            <form class="form-horizontal">
                <div class="box-body">

                    <div class="form-group">
                        <label for="inputEmail3" class="col-sm-2 control-label"><span class="text-danger">*</span>考核名称：</label>

                        <div class="col-sm-4">
                            <input type="text" class="form-control" id="E_Name" name="E_Name" placeholder="">
                        </div>
                    </div>


                    <div class="form-group">
                        <label for="inputEmail3" class="col-sm-2 control-label"><span class="text-danger">*</span>分配班级：</label>
                        <div class="col-sm-4">
                            <select data-placeholder="选择班级" id="AddTeamId" class="chosen-select" multiple style="width: 350px;" tabindex="4">
                                <option value="">请选择班级</option>

                            </select>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="inputEmail3" class="col-sm-2 control-label"><span class="text-danger">*</span>竞赛模式：</label>
                        <div class="col-sm-4">
                            <select data-placeholder="选择模式" id="addType" class="chosen-select" style="width: 350px;" tabindex="4">
                                <option value="">请选择模式</option>
                                <option value="2">练习模式</option>
                                <option value="1">考试模式</option>
                            </select>
                        </div>
                    </div>

                    <div class="form-group">

                        <label for="inputEmail3" class="col-sm-2 control-label"><span class="text-danger">*</span>开始时间：</label>
                        <div class="col-sm-5">
                            <div class="input-group ">

                                <input type="text" id="StartTime" onfocus="WdatePicker({dateFmt:'yyyy-MM-dd H:mm:ss',isShowClear:false,readOnly:false})" class=" form-control" value="">
                            </div>
                        </div>
                    </div>


                    <div class="form-group">

                        <label for="inputEmail3" class="col-sm-2 control-label"><span class="text-danger">*</span>结束时间：</label>
                        <div class="col-sm-5">
                            <div class="input-group ">

                                <input type="text" id="EndTime" onfocus="WdatePicker({dateFmt:'yyyy-MM-dd H:mm:ss',isShowClear:false,readOnly:false})" class=" form-control" value="">
                            </div>
                        </div>
                    </div>

                    <div class="form-group">

                        <label for="inputEmail3" class="col-sm-2 control-label"><span class="text-danger">*</span>练习时长：</label>
                        <div class="col-sm-5">
                            <div class="input-group ">

                                <input type="text" id="PracticeLong"  class=" form-control" value="">
                            </div>
                        </div>
                    </div>

