        <div class="bot_btn_map">
            <div title="接待客户" class="bot_btn bot1" onclick="huanying();"></div>
            <div title="收取单据" class="bot_btn bot2" onclick="shouqdanju()"></div>
            @*<div title="收取资料" class="bot_btn bot2" onclick="ziliaoshouqu()"></div>*@
            <div id="shouquxianjinDiv" title="收取现金" class="bot_btn bot3" onclick="CashCollection()"></div>
            <div title="返还资料" class="bot_btn bot4" onclick="Fanhuankehu()"></div>
            <div title="营销转介" class="bot_btn bot5" onclick="Yinxiaozhuanjie()"></div>
            <div title="送别客户" class="bot_btn bot6" onclick="songbiekehu()"></div>
            <div hidden title="下一个客户" class="bot_btn bot7"></div>
        </div>
        <!--底部右下角按钮结束-->
