                <div class="khmyd">当前客户满意度</div>
                <div class="khmyd_num" id="manyidu">@ViewBag.ErModel.Satisfaction</div>
            </div>


            <div class="dqjdu_map">
                当前任务进度 <button class="jd_btn"></button>
            </div>

            <div id="jd_con" class="jd_con">
                <div class="jd_line">
                    <div class="jd_left">
                        接待
                        <div class="ztweizhi zt_lan" name="jindutiao" data-jindu="7"></div>

                    </div>
                    <div class="jd_right jd_ok"></div>
                </div>

                <div class="jd_line">
                    <div class="jd_left">
                        单据收取
                        <div class="ztweizhi zt_lan" name="jindutiao" data-jindu="8"></div>
                    </div>
                    <div class="jd_right jd_ok"></div>
                </div>

                <div class="jd_line">
                    <div class="jd_left">
