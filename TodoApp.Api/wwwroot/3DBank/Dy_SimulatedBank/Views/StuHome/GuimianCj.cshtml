                    </div>
                    <div class="jd_right"></div>
                </div>




                <div class="jd_line">
                    <div class="jd_left">
                        业务办理
                        <div class="ztweizhi " name="jindutiao" data-jindu="12"></div>
                    </div>
                    <div class="jd_right"></div>
                </div>

                <div class="jd_line">
                    <div class="jd_left">
                        单据打印与盖章
                        <div class="ztweizhi " name="jindutiao" data-jindu="13"></div>
                    </div>
                    <div class="jd_right"></div>
                </div>

                <div class="jd_line">
                    <div class="jd_left">
                        返还资料
                        <div class="ztweizhi " name="jindutiao" data-jindu="14"></div>
                    </div>
                    <div class="jd_right"></div>
                </div>

                <div class="jd_line">
                    <div class="jd_left">
                        送别
                        <div class="ztweizhi " name="jindutiao" data-jindu="15"></div>
                    </div>
                    <div class="jd_right"></div>
                </div>

            </div>





        </div>
        <!--右上角信息样式结束-->
        <!--左侧物品栏-->
        <div id="wupinlan" class="wupin_map">
            <div id="wupin_con" class="wupin__con" ondrop="drop(event)" ondragover="allowDrop(event)" data-type="wupinlan">
                @*<div class="wu1 zanwu"  ondragover="allowDrop(event)" id="div2"></div>
                    <div class="wu2" ondrop="drop2(event)" ondragover="allowDrop2(event)">

                    </div>
                    <div class="wu3" ondrop="drop3(event)" ondragover="allowDrop3(event)">

                    </div>
                    <div class="wu1" ondrop="drop3(event)" ondragover="allowDrop3(event)">

                    </div>*@
            </div>
            <div id="wupin_btn" class="wupin_btn">物 <br /> 品 <br />栏</div>
        </div>
        <!--左侧物品栏结束-->
        <!--各个介质 media-->
        <!--打印机-->
        <div class="jiezhi  jiezhi_dyj"></div>
        <!--文件夹-->
        <div class="jiezhi  jiezhi_wjj" onclick="guiyuantiandan()"></div>
        <!--电脑-->
        <div class="jiezhi  jiezhi_diannao" onclick="mybrain()"></div>

        <!--电话-->
        <div class="jiezhi  jiezhi_dianhua"></div>
        <!--刷卡器-->
        <div class="jiezhi  jiezhi_shuaka"></div>

        <!--验钞机-->
        <div class="jiezhi  jiezhi_ycj" ondrop="drop(event)" ondragover="allowDrop(event)" data-type="yanchaoji"></div>

        <!--扎把机-->
        <div class="jiezhi  jiezhi_zbj"></div>

        <!--印章-->
        <div class="jiezhi  jiezhi_yz" onclick="openyinzhang()"></div>

        <!--款箱-->
        <div class="jiezhi  jiezhi_kx"></div>

        <!--身份证刷卡验证-->
        <div class="jiezhi  jiezhi_sfzyz" ondrop="drop(event)" ondragover="allowDrop(event)" data-type="hechashenfenqi"></div>

        <div id="yinzhanglan" hidden>
            <button type="button" value="1" onclick="clickYinzhang(this)">货币鉴定专用章</button>
            <button type="button" value="2" onclick="clickYinzhang(this)">附件章</button>
            <button type="button" value="3" onclick="clickYinzhang(this)">柜员私章</button>
            <button type="button" value="4" onclick="clickYinzhang(this)">现金收讫章</button>
            <button type="button" value="5" onclick="clickYinzhang(this)">假币章</button>
            <button type="button" value="6" onclick="clickYinzhang(this)">银行业务公章</button>
            <button type="button" value="7" onclick="clickYinzhang(this)">转讫章</button>
            <button type="button" value="8" onclick="clickYinzhang(this)">用户管理专用章</button>
            <button type="button" value="11" onclick="clickYinzhang(this)">签字笔</button>
            <button type="button" onclick="closeYinzhang()">关闭</button>
        </div>

    </div>
</body>
</html>

