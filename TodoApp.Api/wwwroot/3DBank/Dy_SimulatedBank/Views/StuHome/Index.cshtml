    function showImg() {
        $("#unityContainerDiv").show();
        $("#goxit").hide();
        //initUnity();
        kaohe();
    }
    showImg();
    var indexa;
    function changemi() {

        indexa = layer.open({
            type: 1, area: ['500px', '300px'],
            title: "修改密码",
            content: $('#update-pwd')   //这里content是一个DOM，注意：最好该元素要存放在body最外层，否则可能被其它的相对元素所影响
        });
    }
    function changePWd() {
        var pwd1 = $("#newpwd1").val();
        var pwd2 = $("#newpwd2").val();
        if (pwd1 == "" || pwd2 == "") {
            alert("请输入密码");
            return;
        }
        if (pwd1 != pwd2) {
            alert("两次输入密码不一致");
            return;
        }
        $.ajax({
            dataType: "text",
            contentType: "application/json; charset=utf-8",
            url: '/StuHome/ChangePWD',
            data: { "pwd1": pwd1, "pwd2": pwd2 },
            success: function (tb) {
                if (tb == "1") {
                    alert("密码修改成功");
                } else {
                    alert(tb);
                }
                layer.close(indexa);
            }
        });
    }
    TrainingExamination(null, '1', 1);
    // 调用倒计时
    countdown('2021-05-28 17:00:00', function () {
        // 倒计时结束回调
