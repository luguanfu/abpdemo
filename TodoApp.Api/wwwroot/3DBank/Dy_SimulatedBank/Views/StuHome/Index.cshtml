                            <label class="zt_lable" for="item1"></label>
                            <span>进行中</span>
                            <input id="item2" type="radio" name="zt" value="1" onclick="serchinfo()">
                            <label class="zt_lable" for="item2"></label>
                            <span>已完成</span>

                            <!--个人练习按钮-->
                            <span id="grlx">
                                <button type="button" class="btn_pub" onclick="DelAll()">
                                    <img src="../img/public3D/lx_ico_shan.png" />
                                    清空记录
                                </button>
                                <button type="button" class="btn_pub " onclick="openToCaseView();">
                                    <img src="../img/public3D/lx_ico_add.png" />
                                    新增练习
                                </button>
                                <button type="button" class="kuai_go " onclick="GoQuickExercise(1)">
                                    <img src="../img/public3D/lx_ico_gohui.png" />
                                    快速开始
                                </button>
                            </span>

                            <!--团队练习按钮-->
                            <span id="tdlx">
                                @if (ViewBag.Groupingstate == 1)
                                {
                                    <button type="button" class="btn_pub" onclick="DelAll()">
                                        <img src="../img/public3D/lx_ico_shan.png" />
                                        清空记录
                                    </button>
                                    <button type="button" class="btn_pub " onclick="openToCaseView();">
                                        <img src="../img/public3D/lx_ico_add.png" />
                                        新增练习
                                    </button>
                                    <button type="button" class="kuai_go " onclick="GoQuickExercise(2)">
                                        <img src="../img/public3D/lx_ico_gohui.png" />
                                        快速开始
                                    </button>
                                }
                                else if (ViewBag.Groupingstate == 0)
                                {
                                    <button type="button" class="btn_pub " onclick="openToCaseView();">
                                        <img src="../img/public3D/lx_ico_add.png" />
                                        查看案例
                                    </button>
                                }
                            </span>

                        </div>
                        <div class="xinxi_btn" id="ziyoulianxi2" style="display:none;">
                            <input id="zyms1" type="radio" name="zyms" value="0" onclick="serchinfo()" autocomplete="off">
                            <label class="zt_lable" for="zyms1"></label>
                            <span>未开始</span>
                            <input id="zyms2" type="radio" name="zyms" value="1" checked="checked" onclick="serchinfo()" autocomplete="off">
                            <label class="zt_lable" for="zyms2"></label>
                            <span>进行中</span>
