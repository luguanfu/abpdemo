                <button onclick="jiedai();" type="button" class="wan_ok">好的</button>
            </div>
        </div>
    </div>
</div>
<!--对话-->
<div id="duihuakuang" style="display: none;">

    <div class="moshi_con">

        <!--弹窗头部标题加关闭区域-->
        <div class="pub_titele" style="text-align: center;">
            
            <img class="shouqian_guanbi" onclick="layerCloseZdy(this)" style="display:none;" src="/img/public3D/ms_ico_guanbi.png" />
        </div>

        <div class="yxzj_map">
            <p style="margin-top: 50px;" class="text-center" id="all_title">

            </p>
