<script type="text/javascript">

    var openTime = '@ViewBag.ErModel.openTime'
    var endTime = "";
    var serverTime = null;
    var DJSInterval = null;
    //物品栏js
    var divShow = false;
    var withoutMargin = $("#wupin_con").outerWidth();

    var wpnumber = withoutMargin;
    $(function () {
        $("#wupin_btn").click(function () {
            var thisObj = $("#wupinlan");
            if (divShow) {
                thisObj.each(function () {
                    $(this).animate({
                        left: "0"
                    }, 300);
                });
                divShow = false;
            } else {
                thisObj.each(function () {
                    $(this).animate({
                        left: wpnumber
                    }, 300);
                });
                divShow = true;
            }
        });
        $(".jd_btn").click(function () {
            $("#jd_con").slideToggle("slow");
            $(this).toggleClass("qie"); return false;
        });
        var lids = $("#LinkId").val();
        if (lids==4) {
            $(".bot8").attr("onclick", "quhaohouyindao(2)")
        }
        $.ajax({
            url: '/Ashx/TimeHandler.ashx',
            async: false,
            success: function (data) {
                serverTime = Date.parse(data)
            }
        });
        endTime = Date.parse(openTime) + (2*60*60*1000);
        if (serverTime > endTime) {
            layer.msg("已结束", function () { SubmitTask() })
            return;
        }
        DJSInterval = setInterval(setDJS, 1000);
    });
    function fmtDHS(num) {
        if (isNaN(num)) {
            return;
        }
        var h, m, s;
        h = Math.floor(num / (60 * 60 * 1000))
        var mm = num - (h * (60 * 60 * 1000))
        m = Math.floor(mm / (60 * 1000));
        var ss = mm - (m * 60 * 1000)
        s = Math.floor(ss / (1000))
        h += "";
        m += "";
        s += "";
        h = (h.length == 1) ? '0' + h : h;
        m = (m.length == 1) ? '0' + m : m;
        s = (s.length == 1) ? '0' + s : s;
        return h + ":" + m + ":" + s;
    }
    var numTimes = 0;
    function setDJS() {
        serverTime += 1000
        numTimes++
        if (serverTime > endTime) {
            clearInterval(DJSInterval)
            layer.msg("已结束", function () { SubmitTask() })
        }
        var timeStr = fmtDHS(endTime - serverTime);
        $(".timeS").html(timeStr)
        if (numTimes % 60 == 0) {
            $.ajax({
                url: '/Ashx/TimeHandler.ashx',
                async: false,
                success: function (data) {
                    serverTime = Date.parse(data)
                }
            });
        }
    }

    // 挪动方块
    function touch_move(e){
        let box = e.target.parentNode;
        let distanceX = e.clientX - box.offsetLeft;
        let distanceY = e.clientY - box.offsetTop;
        let type = box.className.includes('linkarea'); // true为方块，false是工具栏
        if (type) {
            this.selectItem = e.target.dataset['idx'];
        }
        e.preventDefault();
        // 移动中
        this.$el.onmousemove = function (event) {
            let x = event.clientX - distanceX;
            let y = event.clientY - distanceY;
            let min = (type ? 0 : 10);
            if (x < min) {
                x = min;
            } else if (x > this.offsetWidth - box.offsetWidth - min) {
                x = this.offsetWidth - box.offsetWidth - min;
            }
            if (y < min) {
                y = min;
            } else if (y > this.offsetHeight - box.offsetHeight - min) {
                y = this.offsetHeight - box.offsetHeight - min;
            }
            box.style.left = `${x}px`;
            box.style.top = `${y}px`;
        }.bind(box.parentNode);
        // 鼠标松开
        this.$el.onmouseup = function (event) {
            this.onmousemove = null;
            this.onmouseup = null;
            distanceX = null;
            distanceY = null;
            box = null;
        }
    }
</script>

