}

<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>银行虚拟仿真——接待客户</title>
    <link rel="stylesheet" type="text/css" href="~/CSS/bootstrap.min.css" />
    <link rel="stylesheet" type="text/css" href="~/CSS/font-awesome-4.7.0/css/font-awesome.min.css" />
    <link rel="stylesheet" type="text/css" href="~/CSS/animate.css" />
    <link rel="stylesheet" type="text/css" href="~/CSS/form_public.css" />
    <link rel="stylesheet" type="text/css" href="~/CSS/public3D.css" />
    <link rel="stylesheet" type="text/css" href="~/CSS/3Dstudent.css" />
    <link rel="stylesheet" href="~/CSS/icheck-pub/skins/flat/blue.css">

    <style type="text/css">
        body {
            position: relative;
        }

        .body_map {
            position: relative;
            width: 100%;
            height: 100%;
            height: 100vmin;
        }

        .bodybg {
            width: 100%;
            height: 100%;
        }


        /*介质开始*/

        .jiezhi {
            position: absolute;
            cursor: pointer;
            /*background-color: rgba(0, 0, 0, 0.5);*/
        }

        .jiezhi_quhao {
            /*border: 1px solid yellow;*/
            width: 21%;
            height: 20%;
            left: 7%;
            top: 63%;
            transform: rotate(40deg);
            -ms-transform: rotate(40deg);
            -moz-transform: rotate(40deg);
            -webkit-transform: rotate(40deg);
            -o-transform: rotate(40deg);
        }


        .jiezhi_kehu {
            border: 1px solid green;
            width: 10%;
            height: 47%;
            left: 38%;
            top: 43%;
        }



        .zhiyin4 {
            display: block;
            position: absolute;
            width: 6%;
            height: 8%;
            background: url(/Img/shouzhi.gif) center no-repeat;
            background-size: 100% 100%;
            left: 20%;
            top: 77%;
            z-index: 999999;
        }

        .youxia {
            position: fixed;
            bottom: 30px;
            right: 30px;
        }

        .quhao_map {
            padding: 15px 40px;
        }

        .sbyw {
            font-size: 18px;
        }


        .quhaopai {
            margin: auto;
            font-size: 24px;
            text-align: center;
            border: 1px solid #3487f5;
            padding: 15px;
            margin: 10px auto;
            border-radius: 15px;
            width: 300px;
        }


        .jieguo_map {
            text-align: center;
        }

            .jieguo_map h2 {
                margin-top: 40px;
                color: #3487f5;
            }

        .renwuminxiang {
            position: absolute;
            top: 40px;
            right: 14%;
            width: 400px;
            height: 400px;
            overflow: auto;
            background: #ffffff;
            border-radius: 20px;
            padding: 15px;
            box-shadow: 1px 1px 16px #ffffff;
        }

        .renwuminxiang_t {
            margin-bottom: 10px;
        }
    </style>
</head>
<body>

    <input type="hidden" id="TotalResultId" value="@ViewBag.ErModel.TotalResultId" />
    <input type="hidden" id="ExamId" value="@ViewBag.ErModel.ExamId" />
    <input type="hidden" id="TaskId" value="@ViewBag.ErModel.TaskId" />
    <input type="hidden" id="CustomerId" value="@ViewBag.ErModel.CustomerId" />
    <input type="hidden" id="LinkId" value="@ViewBag.ErModel.LinkId" />
    <input type="hidden" id="Satisfaction" value="@ViewBag.ErModel.Satisfaction" />

    <div class="body_map">
        <img class="ico_jiezhi" src="~/Img/public3D/ico_jeizhi.gif" id="askImg" hidden onclick="askImgClick()" />


        <!--当前模式-->
        <div class="moshi_map">
            当前模式：<span>@ViewBag.ErModel.Type_AllString</span>
            <input type="hidden" id="Type_All" value="@ViewBag.ErModel.Type_All" />
        </div>

        <!--视频和背景图转换区域-->

        <div class="videos_res" id="videoParent">
            <!-- autoplay="autoplay"自动播放 loop="loop"控制循环播放-->
            <video id="flash" preload="metadata" onended="showImg()">
                <source id="videoPath" src="" type="video/mp4">
            </video>

            <img id="bj" class="bodybg" src="" />

            @*<video style="display: none;" id="flash2" loop="loop" autoplay onended="showImg()">
                    <source src="../flash/dome.mp4" type="video/mp4">
                </video>*@
        </div>


        <!--视频和背景图转换区域结束-->
        @*<div class="zhiyin4"></div>*@

        <!--系统设置区域-->
        <div class="shezhi_map">
            <ul class="shezhi_ul">
                <li>
                    <img src="/img/public3D/sy_ico_tuichu.png" />
                    <label class="shuxian"></label>
                </li>
                @if (ViewBag.ErModel.Type_All == "3" && ViewBag.ErModel.UserType == "3")
                {
                    <li>
                        <img src="/img/public3D/sy_ico_shezhi.png" />
                        <label class="shuxian"></label>
                    </li>
                    <li onclick="xxjd();">
                        <img src="/img/public3D/sy_ico_xuexi.png" />
                    </li>
                }
                else
                {
                    <li>
                        <img src="/img/public3D/sy_ico_shezhi.png" />
                    </li>
                }
            </ul>
        </div>
        <!--设置三块内容-->
        <div class="shezhi_con">
            <!--退出实训-->
            <div class="sehzhi_tab">
                <div class="xt_out_map">
                    <div class="xt_out_con">
                        <img class="tc_jiao" src="../img/public3D/sy_ico_sanjiao.png" />
                        <div class="tc_name">
                            退出实训
                        </div>
                        <ul class="tcxt_ul">
                            <li onclick="ExitTask()">退出</li>
                            @if (ViewBag.ErModel.UserType == "3")
                            {
                                <li onclick="SubmitTask()">直接结算成绩</li>
                            }
                        </ul>
                        <div>
                            <button class="tcxt_gb" type="button">关闭</button>
                        </div>
                    </div>
                </div>
            </div>
            <div class="sehzhi_tab">
                <div class="cssz_map">
                    <div class="cssz_con">
                        <img class="cssz_jiao" src="../img/public3D/sy_ico_sanjiao.png" />
                        <div class="cssz_name">
                            环境设置
                        </div>
                        <ul class="cssz_ul">
                            <li>
                                <span>角色配音</span>
                                <label class="switch">
                                    <input type="checkbox" name="SwitchSet" data-name="setpeiyin" checked>
                                    <div class="slider round"></div>
                                </label>
                            </li>
                            <li>
                                <span>操作音效</span>
                                <label class="switch">
                                    <input type="checkbox" name="SwitchSet" data-name="setyinxiao">
                                    <div class="slider round"></div>
                                </label>
                            </li>
                            <li>
                                <span>背景音乐</span>
                                <label class="switch">
                                    <input type="checkbox" name="SwitchSet" data-name="setyinyue">
                                    <div class="slider round"></div>
                                </label>
                            </li>
                            <li>
                                <span>过场动画</span>
                                <label class="switch">
                                    <input type="checkbox" name="SwitchSet" data-name="setdonghua">
                                    <div class="slider round"></div>
                                </label>
                            </li>
                        </ul>
                        <div>
                            <button class="cssz_zhiyin" type="button" onclick="OpenGuide()">操作指引</button>
                        </div>
                        <div>
                            <button class="cssz_gb" type="button">关闭</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!--系统设置区域结束-->
        <!--底部右下角按钮-->
        <div class="bot_btn_map">
            <div title="接待客户" class="bot_btn bot1" onclick="huanying();"></div>
            <div title="引导客户" class="bot_btn bot8" onclick="quhaohouyindao(1)"></div>
            <div title="营销转介" class="bot_btn bot5" onclick="Yinxiaozhuanjie()"></div>
            <div title="送别客户" class="bot_btn bot6" onclick="songbiekehu()"></div>
            <div hidden title="下一位客户" class="bot_btn bot7"></div>
        </div>
        <!--底部右下角按钮结束-->
        <!--右上角信息样式-->
        <div class="xinxi_map">
            <div class="shu_map" style="margin-bottom:0px;">
                <div class="num_rw">
                    任务描述:<span></span>
                </div>
                <div onclick="rwslmap(this);" title="已完成任务数" data-open="on" class="dqnum_rw" id="currTaskCount"><img src="~/Img/public3D/clicSHOU.png" /></div>
                <div class="khmyd">倒计时</div>
                <!--<div class="khmyd_num" id="manyidu">@ViewBag.ErModel.Satisfaction</div>-->
            </div>
            <div class="dqjdu_map timeS" style="margin-bottom:15px;text-align:center;height:40px;line-height:40px;">

            </div>
            <div class="dqjdu_map">
                当前任务进度 <button class="jd_btn"></button>
            </div>
            <div id="jd_con" class="jd_con">
                <div class="jd_line">
                    <div class="jd_left">
                        接待
                        <div class="ztweizhi zt_lan" name="jindutiao" data-jindu="2"></div>

                    </div>
                    <div class="jd_right jd_ok"></div>
                </div>
                <div class="jd_line">
                    <div class="jd_left">
                        接待后引导
                        <div class="ztweizhi zt_lan" name="jindutiao" data-jindu="2.5"></div>

                    </div>
                    <div class="jd_right jd_ok"></div>
                </div>
                <div class="jd_line">
                    <div class="jd_left">
                        取号
                        <div class="ztweizhi zt_lan" name="jindutiao" data-jindu="3"></div>
                    </div>
                    <div class="jd_right jd_ok"></div>
                </div>

                <div class="jd_line">
                    <div class="jd_left">
                        取号后引导
                        <div class="ztweizhi zt_huang" name="jindutiao" data-jindu="4"></div>
                    </div>
                    <div class="jd_right"></div>
                </div>

                <div class="jd_line">
                    <div class="jd_left">
                        填单
                        <div class="ztweizhi " name="jindutiao" data-jindu="5"></div>
                    </div>
                    <div class="jd_right"></div>
                </div>

                <div class="jd_line">
                    <div class="jd_left">
                        填单后引导
                        <div class="ztweizhi " name="jindutiao" data-jindu="6"></div>
                    </div>
                    <div class="jd_right"></div>
                </div>


                <div class="jd_line">
                    <div class="jd_left">
                        送别
                        <div class="ztweizhi " name="jindutiao" data-jindu="16"></div>
                    </div>
                    <div class="jd_right"></div>
                </div>

            </div>
        </div>
        <!--右上角信息样式结束-->
        <!--左侧物品栏-->
        <div id="wupinlan" class="wupin_map" hidden>
            <div id="wupin_con" class="wupin__con">
                <div class="zanwu">暂无物品</div>
            </div>
            <div id="wupin_btn" class="wupin_btn">物品栏</div>
        </div>
        <!--左侧物品栏结束-->
        <!--各个介质 media-->
        <!--取号机-->
        <div onclick="quhao();" class="jiezhi  jiezhi_quhao"></div>




    </div>
    @*任务明细*@
    <div id="renwuminxiang" class="renwuminxiang" style="display:none;">
        <div class="renwuminxiang_t bgview" mousedown="touch_move()">
            任务描述：
        </div>
        <div id="CaseDescription"></div>
    </div>
</body>
</html>

<!--操作指引-->
<div id="zhiyincao" style="display: none;">

    <div onclick="layerCloseZdy(this)" class="btn_guanti"></div>
    <button id="BtnGuidePrev" onclick="GuidePrev();" class="btn btn-primary btn_shang">上一步</button>
    <button id="BtnGuideNext" onclick="GuideNext();" class="btn btn-info btn_xia">下一步</button>

    <img id="buzhoutu" style="width: 100%;height: 100%;" src="" />
</div>


<!--欢迎光临弹窗-->
