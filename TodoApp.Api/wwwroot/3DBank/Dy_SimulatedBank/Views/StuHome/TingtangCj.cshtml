    <div class="moshi_con">

        <!--弹窗头部标题加关闭区域-->
        <div class="pub_titele" style="text-align: center;">
            接待客户
            <img class="shouqian_guanbi" onclick="layerCloseZdy(this)" src="/img/public3D/ms_ico_guanbi.png" />
        </div>

        <div class="yxzj_map">
            <p style="margin-top: 50px;" class="text-center">
